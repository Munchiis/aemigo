{
    "manifest_version": 3,
    "name": "AEMigo",
    "version": "1.0.0",
    "description": "A productivity tool for AEM developers",
    "permissions": [
        "activeTab",
        "scripting",
        "storage",
        "tabs",
        "activeTab",
        "tabGroups"
    ],
    "action": {
        "default_popup": "src/popup/popup.html",
        "default_icon": "aemigo.jpg"
    },
    "options_page": "src/options.html",
    "background": {
        "service_worker": "src/background.js",
        "type": "module"
    }
}